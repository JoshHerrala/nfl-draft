

<%= render 'welcome/menu' %>

<table class="table1">
  <thead>
    <tr>
      <th><h2><%= @team.name %></h2></th>
      <th><% if @team.logo_file_name == nil %>
	  
	  <% else %>
	  <%= image_tag @team.logo.url(:medium) %>
      <%end%></th>
	  <th>Name</th>
      <th>Position</th>
	  <th>Picked in Round:</th>
	  <th>Overall Pick Number:</th>
    </tr>
  </thead>
  <tbody>
    <% @players.each do |player| %>
      <tr>
      <td></td>
        <td></td>
		<td><%= player.name %></td>
        <td><%= player.position %>
		<td><%= player.ownership.round %></td>
		<td><%= player.ownership.pick %></td>
		
      </tr>
    <% end %>
  </tbody>
</table>
