<%= render 'welcome/menu' %>
<%= render 'welcome/admin_menu' %>


<table id="tblMain" class="table1">
  <thead>
    <tr>
    <th><h2>All Players</h2></th>
      <th>Name</th>
      <th>Position</th>
      <th>Team</th>
      <th>Edit</th>
	</tr>
  </thead>
  <tbody>
    <% @players.each do |player| %>
       <tr>
       <td></td>
        <td><%= player.full_name %></td>
		<td><%= player.position %></td>
        <td><%= player.try(:team).try(:name) %> </td> 
  	    <td><%= link_to "Edit Player", edit_player_path(player) %>
		
		<script type="text/javascript">
			   rows()
		</script>
  
  
  <%end%>
  </tbody>
</table>
